{
  "name": "@fhevmsdk/react",
  "version": "0.3.0",
  "description": "React hooks and components for FHEVM SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/0xethll/fhevm-sdk.git"
  },
  "homepage": "https://github.com/0xethll/fhevm-sdk#readme",
  "author": "devasher hlxstc680@gmail.com",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fhevmsdk/core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "wagmi": "^2.0.0",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^19",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "fhevm",
    "react",
    "hooks",
    "encryption",
    "zama"
  ],
  "license": "MIT"
}
