{
  "name": "@fhevm/core",
  "version": "0.1.0",
  "description": "Framework-agnostic FHEVM SDK core",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@zama-fhe/relayer-sdk": "^0.2.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "viem": "^2.0.0"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "zama",
    "confidential",
    "privacy"
  ],
  "license": "MIT"
}
